[user]
	name = sudacode
	email = suda@sudacode.com
	signingkey = /Users/sudacode/.ssh/yuh.pub
[init]
	defaultBranch = main
[push]
	default = simple
	autoSetupRemote = true
[pull]
	rebase = true
[rebase]
	autoStash = true
[fetch]
	prune = true
[diff]
	algorithm = histogram
[merge]
	conflictstyle = zdiff3
[color]
	ui = auto
[core]
	pager = delta
[gpg]
	format = ssh
[commit]
	gpgsign = true
